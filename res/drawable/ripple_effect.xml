    xmlns:tools="http://schemas.android.com/tools"
    android:color="#F3F3F3"
    tools:targetApi="lollipop">
    <item android:id="@android:id/mask">
        <shape 
		    android:shape="rectangle" >
		    <solid android:color="#F3F3F3" />
